@model Kurs.Admin.Models.ScreenResolutionViewModel

@{
    ViewBag.Title = "Удалить разрешение экрана";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Удалить разрешение экрана</h2>

<h3>Вы уверены, что хотите удалить этот элемент?</h3>
<div>
    <h4>Разрешение экрана</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Width)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Width)
        </dd>

    </dl>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Height)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Height)
        </dd>

    </dl>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Diagonal)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Diagonal)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Назад", "Index")
        </div>
    }
</div>
